{
    "name": "artisan-build/conductor",
    "description": "composer package executor.",
    "keywords": ["composer", "conductor", "package", "laravel zero", "console", "cli"],
    "homepage": "https://artisan.build/conductor",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/artisan-build/conductor/issues",
        "source": "https://github.com/artisan-build/conductor"
    },
    "authors": [
        {
            "name": "Len Woodward",
            "email": "Len@ProjektGopher.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "illuminate/http": "^11.5",
        "laravel-zero/framework": "^11.0.0",
        "laravel/prompts": "^0.1.24"
    },
    "require-dev": {
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "Conductor\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/conductor"]
}
